{
  "name": "cdcchen/kawaii",
  "description": "php framework base swoole",
  "type": "framework",
  "license": "BSD",
  "authors": [
    {
      "name": "cdcchen",
      "email": "cdcchen@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6",
    "psr/http-message": "~1.0",
    "psr/log": "~1.0",
    "psr/cache": "~1.0",
    "fig/http-message-util": "^1.1",
    "cdcchen/psr7": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "kawaii\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "require-dev": {
    "phpspec/phpspec": "^3.1",
    "codeception/codeception": "^2.2",
    "codeception/specify": "*",
    "codeception/verify": "*"
  },
  "config": {
    "bin-dir": "bin"
  }
}
